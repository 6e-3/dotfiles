syntax enable
set mouse=a
set clipboard=unnamed
set number
set cursorline
set hlsearch
set incsearch
set listchars=tab:>.,trail:_
set list
set expandtab
set laststatus=2
set statusline=\ %F\ %m%r%h%w%=%{&fileencoding}\ \|\ ^%l>%c\ \|\ %p%%\ 
set wildmenu
highlight Search ctermbg=220 ctermfg=0
highlight lineNr ctermbg=None ctermfg=63
highlight CursorLineNr ctermbg=None ctermfg=63
highlight NonText    ctermbg=None ctermfg=8
highlight SpecialKey ctermbg=None ctermfg=9
highlight StatusLine ctermbg=white ctermfg=63
highlight StatusLine cterm=None ctermbg=63 ctermfg=15
